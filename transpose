class Solution:
    def transpose(self, matrix: List[List[int]]) -> List[List[int]]:
        row = len(matrix)
        col = len(matrix[0])
        ans = [[] for _ in range(col)]
        print(ans)
        for i in range(col):
            for j in range(row):
                ans[i].append(matrix[j][i])
        return ans
        
